{
  "name": "spine-web",
  "version": "1.0.3-SNAPSHOT",
  "license": "Apache-2.0",
  "description": "A JS client for interacting with Spine applications.",
  "homepage": "https://spine.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpineEventEngine/web"
  },
  "bugs": {
    "url": "https://github.com/SpineEventEngine/web/issues",
    "email": "spine-developers@teamdev.com"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack --config webpack-prod.config.js",
    "build-dev": "webpack --config webpack-dev.config.js",
    "transpile-before-publish": "babel main --out-dir build/npm-publication --source-maps",
    "coverage:unix": "nyc --reporter=text-lcov npm run test >| build/coverage.lcov",
    "coverage:win": "nyc --reporter=text-lcov npm run test > build/coverage.lcov",
    "test": "mocha --require @babel/register --recursive --exit --full-trace ./test",
    "license-report": "node ./license-report/generate-license-report-md.js"
  },
  "peerDependencies": {
    "rxjs": "6.5.x"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "google-protobuf": "^3.8.0",
    "isomorphic-fetch": "^2.2.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "codecov": "^3.0.0",
    "firebase": "^6.2.0",
    "license-checker": "^25.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "rxjs": "~6.5.1",
    "sinon": "^7.0.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "sideEffects": true,
  "main": "index.js"
}
